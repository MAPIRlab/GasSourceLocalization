<launch>
 <arg name="scenario" default="C" />
 <arg name="simulation" default="3,5-1_fast" />

 <node pkg="gaden_preprocessing" exec="preprocessing" name="preprocessing" output="screen" >
     #Cell size
     <param name="cell_size" value="0.1"/>
     #CAD models of the enviroment (.stl)
     <param name="number_of_models" value="3"/>
     <param name="model_0" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02-main.stl"/>
     <param name="model_1" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02Outlet002.stl"/>
     <param name="model_2" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02Outlet004.stl"/>

        #CAD model of the outlets (.stl)
     <param name="number_of_outlet_models" value="3"/>
     <param name="outlets_model_0" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02Outlet001.stl"/>
     <param name="outlets_model_1" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02Outlet003.stl"/>
     <param name="outlets_model_2" value="$(find-pkg-share pmfs_env)/$(var scenario)/cad_models/House02Outlet005.stl"/>

        #3D Location of a point in free-space
     <param name="empty_point_x" value="0.0"/>
     <param name="empty_point_y" value="0.0"/>
     <param name="empty_point_z" value="1.0"/>

     <param name="floor_height" value="-0.64"/>
     <param name="worldFile" value="$(find-pkg-share pmfs_env)/$(var scenario)/launch/ros/stage.world"/>

     #Wind Data (the node will append _i.csv to the name that is specified here)
     <param name="uniformWind" value="false"/>
     <param name="wind_files" value="$(find-pkg-share pmfs_env)/$(var scenario)/wind_simulations/$(var simulation)/$(var simulation)"/>

     #Where to write the output files
		<param name="output_path" value="$(find-pkg-share pmfs_env)/$(var scenario)" />
        <param name="generateCoppeliaScene" value="$(var generateCoppeliaScene)" />
    </node>

	<include file="$(find-pkg-share pmfs_env)/navigation_config/conditional_coppelia_launch.py" >
      <arg name="launchCoppelia" value="$(var generateCoppeliaScene)"/>
    </include>  
</launch>