<?xml version="1.0"?>
<package>
  <name>gsl_actionserver</name>
  <version>0.0.1</version>
  <description>Gas Source Localizaton ActionServer. It implements different search strategies: Surge-cast, Spiral, etc.</description>

  <maintainer email="ojedamorala@uma.es">Pepe Ojeda</maintainer>

  <license>GPLv3</license>


  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>move_base</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>gmrf_wind_mapping</build_depend>
  <build_depend>olfaction_msgs</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>move_base</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>gmrf_wind_mapping</run_depend>
  <run_depend>olfaction_msgs</run_depend>


</package>
